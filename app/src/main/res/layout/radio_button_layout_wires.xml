<?xml version="1.0" encoding="utf-8"?>
<vape.val.liquid.util.RadioButtonWithTableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="40dp">

    <TableRow android:id="@+id/tablerow_wires">

        <com.rey.material.widget.RadioButton
            android:id="@+id/radio_1"
            android:checked="true"
            style="@style/Material.Drawable.RadioButton"
            android:layout_weight="1"
            android:text="1" />

        <com.rey.material.widget.RadioButton
            android:id="@+id/radio_wire_2"
            style="@style/Material.Drawable.RadioButton"
            android:layout_weight="1"
            android:text="2" />

        <com.rey.material.widget.RadioButton
            android:id="@+id/radio_wire_3"
            style="@style/Material.Drawable.RadioButton"
            android:layout_weight="1"
            android:text="3" />

        <com.rey.material.widget.RadioButton
            android:id="@+id/radio_wire_4"
            style="@style/Material.Drawable.RadioButton"
            android:layout_weight="1"
            android:text="4" />

        <com.rey.material.widget.RadioButton
            android:id="@+id/radio_wire_5"
            style="@style/Material.Drawable.RadioButton"
            android:layout_weight="1"
            android:text="5" />

        <com.rey.material.widget.RadioButton
            android:id="@+id/radio_wire_6"

            style="@style/Material.Drawable.RadioButton"
            android:text="6" />
    </TableRow>


</vape.val.liquid.util.RadioButtonWithTableLayout>